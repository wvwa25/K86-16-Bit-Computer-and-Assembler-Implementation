.data
fact ?

.code
INPUT           # Loads a user input into R0
SWAP R0, R1     # puts 0 in R0, and the input in R1
LOADI R2, 1     # R1 will store the factorial, initialize to 1

FACTORIAL_LOOP:
    CMP R1, R0  # If R1 == 0, we're done
    JZ DONE

    MULT R2, R1 # R2 *= R1
    SUBI R1, 1  # R1 -= 1
    JMP FACTORIAL_LOOP

DONE:
    STORE R2, fact
    PRINT R2
    HALT


